{
    "workbench.editorAssociations": {
        "*.jar": "default"
    },
    "explorer.confirmDragAndDrop": false,
    "workbench.activityBar.location": "top",
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true,
    "github.copilot.advanced": {
        "authProvider": "github-enterprise"
    },
    "github-enterprise.uri": "https://dnb.ghe.com",
    "github.copilot.enable": {
        "plaintext": true,
        "markdown": true
    },
    "todo-tree.tree.showCountsInTree": true,
    "gitblame.inlineMessageEnabled": true,
    "gitblame.ignoreWhitespace": true,
    "gitblame.statusBarMessageFormat": "Blame ${author.name} (${time.ago} - ${commit.summary})",
    "gitblame.inlineMessageFormat": "Blame ${author.name} (${time.ago} - ${commit.summary})",
    "javascript.referencesCodeLens.enabled": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.implementationsCodeLens.showOnInterfaceMethods": true,
    "typescript.referencesCodeLens.enabled": true,
    "typescript.referencesCodeLens.showOnAllFunctions": true,
    "editor.linkedEditing": true,

    "turboConsoleLog.insertEmptyLineBeforeLogMessage": true,
    "turboConsoleLog.insertEmptyLineAfterLogMessage": true,
    "turboConsoleLog.logMessagePrefix": "",
    "workbench.settings.openDefaultKeybindings": true,
    "cSpell.userWords": [
        "Anbefalt",
        "avkastning",
        "basepath",
        "cfbg",
        "choco",
        "colorfull",
        "conemu",
        "eplatform",
        "eufemia",
        "expl",
        "gcpc",
        "gitblame",
        "gppl",
        "Indeks",
        "Kostnadsoversikt",
        "lokalise",
        "matchmedia",
        "moneymarket",
        "nettleser",
        "nonbinary",
        "Overconsumption",
        "portefølje",
        "Porteføljeanalyse",
        "productclass",
        "pscustomobject",
        "rgba",
        "RGBs",
        "stru",
        "subproject",
        "tanstack",
        "tdone",
        "Teknologi",
        "testid",
        "tgit",
        "transferrables",
        "tthen",
        "typecheck",
        "Uncompress",
        "Unmark",
        "Unprotect",
        "Unpublish",
        "Unrestrict",
        "usehooks",
        "uuidv",
        "Vekst",
        "winget",
        "Wisnes"
    ],
    "errorLens.alignMessage": {
        "end": 0,
        "minimumMargin": 0,
        "start": 150
    },
    "errorLens.fontWeight": "100",
    "github.copilot.chat.generateTests.codeLens": true,
    "editor.tabSize": 2,
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.useConstantStrings": true,
    "powershell.codeFormatting.useCorrectCasing": true,
    "editor.guides.bracketPairs": "active",
    "editor.bracketPairColorization.enabled": true,

    "errorLens.hintGutterIconColor": "#2faf64",
    "errorLens.infoGutterIconColor": "#a075ff",
    "errorLens.warningGutterIconColor": "#cfa900",
    "errorLens.errorGutterIconColor": "#cf0000",
    "window.newWindowDimensions": "maximized",
    "workbench.colorTheme": "RedSpecter",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.rulers": [
        120
    ],
    "editor.mouseWheelZoom": true,
    "window.zoomLevel": -1, // Default zoom level for the entire window
    "editor.fontSize": 17,  // Default font size for the editor
}
